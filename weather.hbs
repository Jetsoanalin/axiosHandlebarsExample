<table border = "2">
<thead><tr><th title="Field #1">longitudes</th>
<th title="Field #2">latitudes</th>
<th title="Field #3">weather id</th>
<th title="Field #4">weather main</th>
<th title="Field #5">weather description</th>
<th title="Field #6">weather icon</th>
<th title="Field #7">base</th>
